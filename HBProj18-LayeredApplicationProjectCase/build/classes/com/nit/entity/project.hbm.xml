<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nit.entity.Project" table="PROJECT">
	<id name="projId" column="PROJID" />
	<property name="projName" column="PROJNAME"></property>
	<property name="teamSize" column="TEAMSIZE"></property>
	<property name="domain" column="DOMAIN"></property>
	<property name="sdop" column="SDOP"></property>
	<property name="edop" column="CDOP"></property>
	</class>

</hibernate-mapping>