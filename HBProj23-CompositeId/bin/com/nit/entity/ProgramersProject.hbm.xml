<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        <class name="com.nit.entity.ProgramersProject" table="programers_project">
        <composite-id name="id" class="com.nit.entity.ProgramerProjectId">
        <key-property name="programerId" column="PROGRAMERID"></key-property>
        <key-property name="projectId" column="PROJECTID"></key-property>
        
        </composite-id>
        <property name="programerName" column="PROGRAMERNAME"></property>
        <property name="projectName" column="PROJECTNAME"></property>
        <property name="salary" column="SALARY"></property>
        
        
        
        </class>
        
        </hibernate-mapping>