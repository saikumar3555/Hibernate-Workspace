<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
     <class name="com.nt.entity.Project"  table="PROJECT">
       <id name="projId" column="PROJID" type="int"/>
       <property name="projName" column="PROJNAME" type="string" length="15"/>
       <property name="teamSize" column="TEAMSIZE" type="int" length="2"/>
       <property name="domain" column="DOMAIN" type="string" length="10"/>
       <property name="sdp" column="SDP" type="date" />
       <property name="edcp" column="EDCP" type="date" />
     </class>
  </hibernate-mapping>        